{
    "name": "dianxiaomi/dianxiaomi",
    "description": "WordPress Plugin",
    "type": "project",
    "require": {
        "wp-coding-standards/wpcs": "3.2.0",
        "squizlabs/php_codesniffer": "^3.8",
        "automattic/vipwpcs": "^3.0.1",
        "vena/php-cs-fixer-wordpress": "dev-main"
    },
    "license": "GPL",
    "autoload": {
        "psr-4": {
            "dianxiaomi\\dianxiaomi\\": "src/",
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Maxime Michaud",
            "email": "14856114+MaximeMichaud@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "tareq1988/wp-php-cs-fixer": "dev-master",
        "phpstan/phpstan": "^1.10",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "php-stubs/woocommerce-stubs": "^10.3",
        "php-stubs/wordpress-stubs": "^6.0",
        "vimeo/psalm": "^5.0",
        "humanmade/psalm-plugin-wordpress": "^3.0"
    }
}
